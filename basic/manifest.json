{
    "manifest_version": 2,
    "name": "learning extension",
    "version": "1.0",
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "yahi.png"
    }
}